{
  "name": "Noir Vira",
  "type": "dark",
  "colors": {
    "editor.background": "#050505",
    "editor.foreground": "#f5f5dc",
    "editor.selectionBackground": "#444444cc",
    "editor.lineHighlightBackground": "#222222",
    "editorCursor.foreground": "#FF9500",
    "editorWhitespace.foreground": "#3c3c3c",

    "tab.activeBackground": "#050505",
    "tab.inactiveBackground": "#050505",
    "tab.activeForeground": "#ffffff",
    "tab.inactiveForeground": "#6b6b6b",
    "tab.border": "#050505",
    "tab.activeBorder": "#ffb833",

    "editorGroupHeader.tabsBackground": "#050505",
    "tab.hoverBackground": "#111111",

    "activityBar.background": "#050505",
    "activityBar.foreground": "#ffffff",
    "activityBar.activeBorder": "#ffb833",
    "sideBar.background": "#050505",
    "sideBar.foreground": "#cccccc",
    "sideBarSectionHeader.background": "#050505",
    "sideBarSectionHeader.foreground": "#6b6b6b",

    "statusBar.background": "#050505",
    "statusBar.foreground": "#ffffff",
    "statusBar.noFolderBackground": "#050505",
    "statusBar.debuggingBackground": "#803030",

    "titleBar.activeBackground": "#050505",
    "titleBar.activeForeground": "#ffffff",
    "titleBar.inactiveBackground": "#121212",
    "titleBar.inactiveForeground": "#888888",

    "dropdown.background": "#050505",
    "dropdown.foreground": "#f5f5dc",
    "scrollbarSlider.background": "#3c3c3c",

    "badge.background": "#ffb833",
    "badge.foreground": "#050505",

    "statusBarItem.remoteBackground": "#1b1613",
    "statusBarItem.remoteForeground": "#e0b000",
    "statusBarItem.remoteHoverBackground": "#ffcc00",
    "statusBarItem.remoteHoverForeground": "#792447",

    "button.background": "#ffb833",
    "button.foreground": "#1a1a1a",
    "button.hoverBackground": "#ffda44",
    "button.border": "#daa520"
  },

  "tokenColors": [
    {
      "name": "Keywords",
      "scope": ["keyword", "keyword.control", "keyword.operator", "keyword.other"],
      "settings": {
        "foreground": "#FFB833",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "storage.type.primitive",
        "support.type",
        "entity.name.type",
        "support.class",
        "meta.type.cast.expr",
        "meta.return-type"
      ],
      "settings": {
        "foreground": "#f53747",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function",
        "meta.function-call.python",
        "support.function.builtin",
        "support.function.builtin.python",
        "entity.name.function.constructor"
      ],
      "settings": {
        "foreground": "#7FB4F0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.parameter",
        "meta.definition.variable.name",
        "support.variable",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#f5f5dc"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.quoted.double",
        "string.quoted.single",
        "string.template"
      ],
      "settings": {
        "foreground": "#9BC27D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language.boolean", "constant.language.null"],
      "settings": {
        "foreground": "#c85e60"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "comment.line", "comment.block", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#3e3e3e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operators and Punctuation",
      "scope": [
        "keyword.operator",
        "punctuation",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition",
        "punctuation.definition.string",
        "punctuation.section"
      ],
      "settings": {
        "foreground": "#FFB833"
      }
    },
    {
      "name": "HTML Tags and Attributes",
      "scope": [
        "entity.name.tag",
        "meta.tag",
        "meta.tag.sgml",
        "entity.other.attribute-name",
        "string.quoted.double.html"
      ],
      "settings": {
        "foreground": "#E8A87C",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class and Struct",
      "scope": [
        "entity.name.type.class",
        "entity.name.type.struct",
        "meta.class",
        "meta.struct"
      ],
      "settings": {
        "foreground": "#c678dd",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Constants and Enums",
      "scope": [
        "constant",
        "constant.language",
        "constant.other",
        "variable.other.constant",
        "entity.name.constant",
        "entity.name.enum"
      ],
      "settings": {
        "foreground": "#d19a66"
      }
    },
    {
      "name": "Markdown Specific",
      "scope": [
        "markup.heading",
        "markup.bold",
        "markup.italic",
        "markup.inline.raw",
        "markup.quote",
        "markup.list"
      ],
      "settings": {
        "foreground": "#ffcc00"
      }
    },
    {
    "name": "Extended Language Support",
    "scope": [
      "keyword.mnemonic",
      "meta.instruction",
      "support.constant.register",
      "constant.numeric.hex",

      "variable.other.readwrite.ruby",
      "constant.language.ruby",
      "entity.name.function.ruby",
      "support.function.kernel",
      "meta.class.ruby",
      "punctuation.definition.block.ruby",

      "variable.other.perl",
      "support.function.perl",
      "constant.language.perl",
      "punctuation.definition.variable.perl",

      "entity.name.function.zig",
      "storage.type.zig",
      "keyword.zig",
      "variable.zig",

      "keyword.control.cs",
      "entity.name.type.class.cs",
      "variable.other.cs",
      "support.class.system",
      "entity.name.namespace.cs",
      "storage.type.cs",

      "keyword.control.asp",
      "variable.language.asp",
      "support.function.asp",
      "meta.function-call.asp",

      "source.shell",
      "variable.language.shell",
      "string.interpolated.shell",
      "support.function.builtin.shell",

      "keyword.control.makefile",
      "entity.name.function.makefile",
      "variable.other.makefile",

      "variable.parameter.r",
      "support.function.r",
      "entity.name.function.r",
      "constant.language.r",

      "entity.name.function.dart",
      "support.class.dart",
      "storage.type.dart",
      "variable.other.dart",

      "keyword.control.kotlin",
      "variable.parameter.kotlin",
      "entity.name.class.kotlin",
      "meta.function.kotlin"
    ],
    "settings": {
      "foreground": "#f0a0ff",
      "fontStyle": "bold"
    }
  },
{
  "name": "PowerShell Enhanced",
  "scope": [
    "keyword.control.powershell",
    "keyword.operator.powershell",
    "keyword.other.powershell",
    "variable.language.powershell",
    "variable.other.readwrite.powershell",
    "support.function.powershell",
    "support.constant.powershell",
    "constant.numeric.powershell",
    "string.quoted.single.powershell",
    "string.quoted.double.powershell",
    "comment.line.number-sign.powershell",
    "entity.name.function.powershell",
    "storage.type.powershell"
  ],
  "settings": {
    "foreground": "#569CD6",
    "fontStyle": "bold"
  }
}


  ]
}
